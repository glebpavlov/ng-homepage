{
  "name": "ng-homepage",
  "version": "1.0.1",
  "scripts": {
    "postinstall": "husky install",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "lint": "ng lint",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "release": "standard-version",
    "analyze": "npm run build:prod -- --stats-json --named-chunks && webpack-bundle-analyzer ./dist/ng-homepage/stats.json",
    "dev:ssr": "ng run ng-homepage:serve-ssr",
    "serve:ssr": "node dist/ng-homepage/server/main.js",
    "build:ssr": "ng build && ng run ng-homepage:server",
    "prerender": "ng run ng-homepage:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.2",
    "@angular/cdk": "^17.0.1",
    "@angular/common": "^17.0.2",
    "@angular/compiler": "^17.0.2",
    "@angular/core": "^17.0.2",
    "@angular/forms": "^17.0.2",
    "@angular/platform-browser": "^17.0.2",
    "@angular/platform-browser-dynamic": "^17.0.2",
    "@angular/platform-server": "^17.0.2",
    "@angular/router": "^17.0.2",
    "@angular/ssr": "^17.0.1",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@ng-web-apis/common": "^3.0.6",
    "@ng-web-apis/intersection-observer": "^3.1.6",
    "@ng-web-apis/universal": "^3.0.7",
    "@ngrx/component": "^16.3.0",
    "@ngrx/effects": "^16.3.0",
    "@ngrx/router-store": "^16.3.0",
    "@ngrx/store": "^16.3.0",
    "@ngrx/store-devtools": "^16.3.0",
    "@taiga-ui/addon-doc": "^3.55.0",
    "@taiga-ui/addon-mobile": "^3.55.0",
    "@taiga-ui/cdk": "^3.55.0",
    "@taiga-ui/core": "^3.55.0",
    "@taiga-ui/icons": "^3.55.0",
    "@taiga-ui/kit": "^3.55.0",
    "@taiga-ui/styles": "^3.55.0",
    "@types/marked": "^4.3.0",
    "express": "^4.18.2",
    "marked": "^4.3.0",
    "ngx-markdown": "^17.1.0",
    "rxjs": "^7.8.1",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.0",
    "@angular-builders/jest": "^13.0.2",
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular-eslint/builder": "^17.0.1",
    "@angular-eslint/eslint-plugin": "^17.0.1",
    "@angular-eslint/eslint-plugin-template": "^17.0.1",
    "@angular-eslint/schematics": "^17.0.1",
    "@angular-eslint/template-parser": "^17.0.1",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.1",
    "@babel/eslint-plugin": "^7.22.10",
    "@commitlint/cli": "^18.4.1",
    "@commitlint/config-conventional": "^18.4.0",
    "@ngneat/spectator": "^15.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "angular-in-memory-web-api": "^0.17.0",
    "eslint": "^8.53.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-ngrx": "^2.1.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-preset-angular": "^13.1.4",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "standard-version": "^9.5.0",
    "typescript": "~5.2.2",
    "webpack-bundle-analyzer": "^4.9.1"
  },
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "build",
        "section": "Build System"
      }
    ]
  }
}
